{
  "name": "ironmental-backend",
  "version": "1.0.0",
  "description": "Serverless webpack example using ecma script",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/jest",
    "eslint": "./node_modules/.bin/eslint src/**",
    "offline": "sls offline --port 5001 -c",
    "dev": "NODE_PATH=src APP_ENV=local nodemon src/local --exec babel-node",
    "deploy:dev": "sls deploy --stage dev",
    "deploy": "sls deploy --stage prod",
    "offline-cron": "sls offline start -c --location .webpack/service"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-source-map-support": "^2.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.7.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "serverless": "^1.45.1",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.4.2",
    "serverless-offline-scheduler": "^0.3.7",
    "serverless-webpack": "^5.3.1",
    "webpack": "^4.35.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "aws-sdk": "^2.479.0",
    "express": "^4.17.1",
    "mongoose": "^5.5.12",
    "nodemailer": "^6.2.1",
    "source-map-support": "^0.5.12",
    "striptags": "^3.1.1"
  }
}
